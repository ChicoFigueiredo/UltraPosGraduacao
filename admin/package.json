{
  "name": "ngx-admin",
  "version": "2.3.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akveo/ngx-admin.git"
  },
  "bugs": {
    "url": "https://github.com/akveo/ngx-admin/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint ngx-admin-demo --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "build-dev-up": "ng build --prod --aot --configuration=dev-server && node ../util/deploy/deploy.js --dev --admin",
    "build-dev-up-fast": "ng build --prod --aot --configuration=dev-server && node ../util/deploy/deploy.js --dev --admin --fast",
    "build-dev-up-content": "ng build --prod --aot --configuration=dev-server && node ../util/deploy/deploy.js --dev --admin --content",
    "build-hmp-up": "ng build --prod --aot --configuration=hmp && node ../util/deploy/deploy.js --hmp --admin",
    "build-hmp-up-fast": "ng build --prod --aot --configuration=hmp && node ../util/deploy/deploy.js --hmp --admin --fast",
    "build-hmp-up-content": "ng build --prod --aot --configuration=hmp && node ../util/deploy/deploy.js --hmp --admin --content",
    "build-prod-up": "ng build --prod --aot --configuration=prod && node ../util/deploy/deploy.js --prod --admin",
    "build-prod-up-fast": "ng build --prod --aot --configuration=prod && node ../util/deploy/deploy.js --prod --admin --fast",
    "build-prod-up-content": "ng build --prod --aot --configuration=prod && node ../util/deploy/deploy.js --prod --admin --content",
    "build-dev": "ng build --prod --aot --configuration=dev-server",
    "build-hmp": "ng build --prod --aot --configuration=hmp",
    "build-prod": "ng build --prod --aot --configuration=prod",
    "dev-up": "node ../util/deploy/deploy.js --dev --admin",
    "dev-up-fast": "node ../util/deploy/deploy.js --dev --admin --fast",
    "dev-up-content": "node ../util/deploy/deploy.js --dev --admin --content",
    "hmp-up": "node ../util/deploy/deploy.js --hmp --admin",
    "hmp-up-fast": "node ../util/deploy/deploy.js --hmp --admin --fast",
    "hmp-up-content": "node ../util/deploy/deploy.js --hmp --admin --content",
    "prod-up": "node ../util/deploy/deploy.js --prod --admin",
    "prod-up-fast": "node ../util/deploy/deploy.js --prod --admin --fast",
    "prod-up-content": "node ../util/deploy/deploy.js --prod --admin --content"
  },
  "dependencies": {
    "@agm/core": "1.0.0-beta.5",
    "@angular/animations": "6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@asymmetrik/ngx-leaflet": "4.0.0",
    "@nebular/auth": "2.0.1",
    "@nebular/bootstrap": "^2.0.1",
    "@nebular/security": "^2.0.1",
    "@nebular/theme": "2.0.1",
    "@ng-bootstrap/ng-bootstrap": "3.2.2",
    "@ng-select/ng-select": "^2.10.1",
    "@swimlane/ngx-charts": "9.0.0",
    "angular-tree-component": "8.0.0",
    "angular2-chartjs": "0.5.1",
    "angular2-toaster": "6.1.0",
    "bootstrap": "4.1.3",
    "chart.js": "^2.7.2",
    "ckeditor": "4.10.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "echarts": "^4.1.0",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "leaflet": "1.3.4",
    "nebular-icons": "1.1.0",
    "ng2-ckeditor": "1.2.1",
    "ng2-completer": "^2.0.8",
    "ng2-file-upload": "^1.3.0",
    "ng2-smart-table": "1.3.5",
    "ngx-echarts": "4.0.0",
    "ngx-mask": "^6.4.12",
    "normalize.css": "8.0.0",
    "objectid-purejs": "^1.0.1",
    "pace-js": "1.0.2",
    "roboto-fontface": "0.10.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "socicon": "3.0.5",
    "tinymce": "4.8.3",
    "typeface-exo": "0.0.61",
    "web-animations-js": "2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.4",
    "@angular/cli": "6.2.4",
    "@angular/compiler-cli": "6.1.9",
    "@angular/language-service": "6.1.9",
    "@compodoc/compodoc": "^1.1.7",
    "@types/d3-color": "1.2.1",
    "@types/googlemaps": "3.30.13",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/leaflet": "1.2.11",
    "@types/node": "10.11.4",
    "codelyzer": "4.5.0",
    "conventional-changelog-cli": "2.0.5",
    "debug": "^4.0.1",
    "jasmine-core": "3.2.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "npm-run-all": "4.1.3",
    "protractor": "^5.4.1",
    "rimraf": "2.6.2",
    "stylelint": "9.6.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-language-service": "0.9.9",
    "typescript": "2.7.2"
  }
}
